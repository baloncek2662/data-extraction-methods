    <occurrences value="4"/>
    <collapsingbt value="false"/>
    <ambiguity value="1"/>
    <freePlus value="false"/>
    <LLk value="5"/>
    <RRk value="5"/>
    <!-- labeling -->
    <prefixLabelling value="true"/>
    <trimLabels value="true"/>
    <!-- output -->
    <dataset_style value="roadrunner/marshall/style/data.xsl"/>
    <index_style value="roadrunner/marshall/style/index.xsl"/>
    <browser value="mozilla"/>
</preferences>

